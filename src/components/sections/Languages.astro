---
import Section from "@/components/Section.astro";
import { languages } from "@cv";
---

<Section title="Languages">
  <ul>
    {
      languages.map(({ language, fluency }) =>  <li><p>{language} <small>({fluency})</small></p></li>)
    }
  </ul>
</Section>

<style>
  ul li small {
    color: #999;
  }
</style>
